{
  "name": "@seanmozeik/s3up",
  "version": "0.3.1",
  "description": "Upload files to S3-compatible storage",
  "type": "module",
  "bin": {
    "s3up": "src/index.ts"
  },
  "files": [
    "src",
    "package.json"
  ],
  "keywords": [
    "s3",
    "upload",
    "cli",
    "bun",
    "r2",
    "cloudflare",
    "aws",
    "backblaze"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanmozeik/s3up.git"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "boxen": "^8.0.1",
    "figlet": "^1.10.0",
    "gradient-string": "^3.0.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "1.3.8",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --compile --outfile s3up --minify",
    "bundle": "bun build ./src/index.ts --outdir dist --target=bun --bytecode --minify",
    "check": "biome check --write --unsafe .",
    "dev": "bun run src/index.ts",
    "format": "biome format --write .",
    "install-local": "bun run build && mkdir -p ~/.local/bin && mv s3up ~/.local/bin/s3up && chmod +x ~/.local/bin/s3up",
    "tc": "tsc --noEmit",
    "test": "bun test"
  }
}
